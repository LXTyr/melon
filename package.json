{
  "name": "melon",
  "version": "0.3.6",
  "description": "ui components for react",
  "main": "./src",
  "directories": {
    "doc": "doc",
    "test": "test",
    "example": "example"
  },
  "devDependencies": {
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-external-helpers-insert-require": "^1.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "browserify-istanbul": "^2.0.0",
    "coveralls": "^2.11.12",
    "css-loader": "^0.24.0",
    "esdoc": "^0.4.8",
    "esdoc-es7-plugin": "0.0.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fecs": "^0.8.6",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-babel-external-helpers": "^2.0.2",
    "gulp-clean": "^0.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "html-webpack-harddisk-plugin": "0.0.2",
    "html-webpack-plugin": "^2.22.0",
    "jasmine": "^2.4.1",
    "jasmine-expect-jsx": "^1.1.2",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^5.1.0",
    "karma-browserstack-launcher": "^1.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "nib": "^1.1.2",
    "numen": "^0.2.0",
    "react-addons-test-utils": "^0.14.8",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "watchify": "^3.7.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2"
  },
  "scripts": {
    "start": "node tools/server.js",
    "build": "rimraf lib && gulp build",
    "lint": "fecs src",
    "docs": "esdoc -c esdoc.json",
    "setup": "rimraf asset && webpack --config=tools/webpack.dll.js",
    "test": "./node_modules/.bin/karma start ./test/karma.conf.js",
    "test-ci": "./node_modules/.bin/karma start ./test/karma.ci.conf.js",
    "coveralls": "cat ./coverage/lcov/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-melon/melon.git"
  },
  "peerDependencies": {
    "react": ">=0.14"
  },
  "keywords": [
    "react",
    "ui",
    "components"
  ],
  "author": "ludafa@outlook.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/react-melon/melon/issues"
  },
  "homepage": "https://github.com/react-melon/melon#readme",
  "edp": {
    "wwwroot": "/",
    "depDir": "dep",
    "srcDir": "src",
    "loaderAutoConfig": "js,htm,html,tpl,vm,phtml",
    "loaderUrl": "http://s1.bdstatic.com/r/www/cache/ecom/esl/2-1-4/esl.js",
    "dependencies": {}
  },
  "dependencies": {
    "melon-core": "^0.3.2",
    "react": "^0.14.8",
    "react-dom": "0.14.8",
    "react-motion": "^0.4.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ]
  },
  "babelBuild": {
    "presets": [
      [
        "es2015",
        {
          "loose": true,
          "modules": "umd"
        }
      ],
      "stage-2",
      "react"
    ],
    "plugins": [
      "external-helpers",
      "external-helpers-insert-require",
      "transform-es3-property-literals",
      "transform-es3-member-expression-literals"
    ]
  }
}
