{
  "name": "melon",
  "version": "0.1.3",
  "description": "ui components for react",
  "main": "./src",
  "directories": {
    "doc": "doc",
    "test": "test",
    "example": "example"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "expect": "^1.12.2",
    "expect-jsx": "^2.0.1",
    "isparta": "^3.5.3",
    "jsdom": "^7.0.2",
    "lodash": "^3.10.1",
    "mime": "^1.3.4",
    "mocha": "^2.2.5",
    "nib": "^1.1.0",
    "react-addons-test-utils": "^0.14.3",
    "react-remarkable": "^1.1.1",
    "react-tap-event-plugin": "^0.2.1",
    "react-tools": "^0.13.3",
    "remarkable": "^1.6.0",
    "stylus": "^0.52.0",
    "swig": "^1.4.2"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha test/components --compilers js:babel/register --require ./test/setup.js",
